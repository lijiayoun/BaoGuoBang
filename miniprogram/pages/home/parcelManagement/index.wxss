/* pages/home/parcelManagement/index.wxss */
.parcel-get{
  padding: 0 30rpx;
}

.item-image{
    width: 40rpx;
    flex-direction: row;
    align-items: center;
    height: 80rpx;
}
.parcel-get navigator {
    display: flex;
     justify-content:space-between;
     padding: 30rpx 0;
     border-bottom: 1rpx solid #e8e8e8;
}
.parcel-get navigator text{
    width: 100%;
    padding: 18rpx 0;
    background-color: rgb(255, 188, 2);
    display: flex;
    font-size: 50rpx;
    color: #fff;
    align-items: center;
    justify-content: center;
    border-radius: 10rpx;
}
.parcel-publish{
  padding: 0 30rpx;
}
.parcel-publish navigator {
    display: flex;
     justify-content:space-between;
     padding: 30rpx 0;
     border-bottom: 1rpx solid #e8e8e8;
}
.parcel-publish navigator text{
    width: 100%;
    padding: 18rpx 0;
    background-color: rgb(255, 61, 2);
    display: flex;
    font-size: 50rpx;
    color: #fff;
    align-items: center;
    justify-content: center;
    border-radius: 10rpx;
}