/* pages/setUp/setUp.wxss */
.setup-list {
    padding: 0 30rpx;
}
.setup-list navigator {
    display: flex;
     justify-content:space-between;
     padding: 30rpx 0;
     border-bottom: 1rpx solid #e8e8e8;
}
.setup-list navigator text{
    font-size: 30rpx;
}
.setup-list navigator image{
    height: 35rpx;
    width: 35rpx;
}
.quit {
  padding: 30rpx ;
}
.quit text{
    width: 100%;
    padding: 18rpx 0;
    background-color: rgba(217, 42, 42, 1);
    display: flex;
    font-size: 30rpx;
    color: #fff;
    align-items: center;
    justify-content: center;
    border-radius: 10rpx;

}
.head{
    overflow: hidden;
    border-radius: 20%;
    width: 200rpx;
    height: 200rpx;
    margin: 16rpx 60rpx;
}
.name{
    line-height: 60rpx;
    width: 208rpx;
    height: 210rpx;
    margin: 100rpx 50rpx;
    font-size: 96rpx;
}
.float{
    float: left;
    
}
.item-image{
    width: 40rpx;
    flex-direction: row;
    align-items: center;
    height: 80rpx;
}